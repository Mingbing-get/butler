{
  "name": "@ai-nucl/server-tool-database",
  "version": "1.0.0",
  "description": "AI Nucl server tool database functionality",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@ai-nucl/server-ai": "workspace:*",
    "node-sql-parser": "^5.3.13"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.11.0"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/cjs/index.cjs",
    "module": "dist/es/index.mjs",
    "types": "dist/types/index.d.ts",
    "type": "module",
    "exports": {
      ".": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/es/index.mjs",
        "require": "./dist/cjs/index.cjs"
      }
    }
  }
}
